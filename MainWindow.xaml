<Window x:Class="DisasmUiTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DisasmUiTest"
        mc:Ignorable="d"
        Title="Disasm UI Test" Height="300" Width="400">
    <StackPanel>
        <Button Name="simpleList" Content="Simple List"
                Width="200" Height="50" Margin="8" Click="SimpleList_Click"/>
        <Button Name="multiColTemplate" Content="Multi-Column Template"
                Width="200" Height="50" Margin="8" Click="MultiColTemplate_Click"/>
        <Button Name="rowPresenter" Content="GridViewRowPresenter"
                Width="200" Height="50" Margin="8" Click="RowPresenter_Click"/>
        <Button Name="full" Content="Fully Styled"
                Width="200" Height="50" Margin="8" Click="FullyStyled_Click"/>
    </StackPanel>
</Window>
